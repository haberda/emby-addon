{
  "name": "emby",
  "url": "https://github.com/haberda/emby-addon",
  "version": "dev",
  "slug": "emby",
  "privileged": ["SYS_ADMIN", "DAC_READ_SEARCH"],
  "apparmor": true,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "description": "emby media server",
  "startup": "system",
  "boot": "manual",
  "map": [
    "share:rw",
    "media:rw",
    "ssl:rw"
  ],
  "devices": [
    "/dev/dri",
    "/dev/vchiq",
    "/dev/video10",
    "/dev/video11",
    "/dev/video12"
  ],
  "ports": {
    "8096/tcp": 8096,
    "8920/tcp": 8920,
    "33791/tcp": 33791,
    "33796/tcp": 33796,
    "7359/udp": 7359,
    "1900/udp": 1900
  },
  "ports_description": {
    "8096/tcp": "http webui",
    "8920/tcp": "https webui",
    "33791/tcp": "external http webui",
    "33796/tcp": "external https webui",
    "7359/udp": "7359",
    "1900/udp": "1900"
  },
  "options": {
    "networkdisks": [],
    "cifsusername": "<username>",
    "cifspassword": "<password>",
    "cifsversion": "3.0"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "networkdisks": ["str"],
    "cifsusername": "str",
    "cifspassword": "str",
    "cifsversion": "list(3.0|2.1|2.0|1.0)?"
  },
  "webui": "http://[HOST]:[PORT:8096]",
  "backup_exclude":["*/cache/", "*/logs/", "*/transcoding-temp/", "*/metadata/"],
  "image": "ghcr.io/haberda/emby/{arch}"
}
